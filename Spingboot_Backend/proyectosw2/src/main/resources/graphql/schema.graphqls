type Query {
    login (email:String,password:String):Usuario
    getCategoriaById(id: String): CategiriaEntity
    getCuentaById(usuarioID: ID): [CuentaEntity]
    Egreso(id: String): EgresoEntity

}
type Mutation{
    createCuenta(  id:ID , nombre:String, saldo:String ,usuarioID:String):String
}




type Usuario {
    id: String
    nombre: String
    email: String
    password: String
}

type CategiriaEntity {
    id: String
    nombre: String
}
 type CuentaEntity{
 id :String
  nombre:String
  saldo:String
  usuarioID:ID
 }
type EgresoEntity {
      id:String
      concepto:String
      monto:String
      fechaEgreso:String
      CuentaID:String
      CategoriaID:String
}


